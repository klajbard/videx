{
  "name": "videx",
  "version": "0.0.1",
  "description": "Video library dashboard",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prisma:generate": "npx prisma generate",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:dev:migrate": "DATABASE_URL='file:databases/dev.db' npx prisma migrate dev",
    "dev": "tsx --tsconfig server/tsconfig.json server/src/index.ts",
    "typecheck": "tsc --noEmit -p server/",
    "check": "biome check ./shared ./server ./prisma",
    "format": "biome check ./shared ./server ./prisma *.ts biome.json package.json --fix --unsafe",
    "test": "vitest"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@prisma/client": "^6.13.0",
    "fastify": "^5.4.0",
    "fastify-type-provider-zod": "^5.0.2",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@faker-js/faker": "^9.9.0",
    "@types/node": "^24.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
